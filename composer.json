{
    "name": "tomkyle/transposer",
    "description": "A PHP library for transposing arrays and objects.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "transpose",
        "array",
        "transpose array",
        "array transpose"
      ],
    "require": {
        "php": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "tomkyle\\Transposer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "tomkyle/find-run-test": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.67",
        "rector/rector": "^2.0",
        "phpstan/phpstan": "^2.1"
    }

}
